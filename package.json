{
  "name": "lib-ui-components",
  "version": "1800.0.0",
  "scripts": {
    "-----BASIC-----": "exit 0",
    "start": "ng serve",
    "test": "ng test",
    "build": "ng build --configuration production",
    "sync": "npm run git:sync:branches",
    "-----CODE-GLANCE-----": "exit 0",
    "eslint": "npx eslint --fix .",
    "prettier": "npx prettier --write .",
    "format": "npm run prettier && npm run eslint",
    "-----SCRIPTS-----": "exit 0",
    "scripts:grant:access": "chmod -R +x scripts/\n",
    "-----HUSKY-----": "exit 0",
    "husky:setup": "husky && node husky-mod.js --moveHusky false",
    "-----NPM-----": "exit 0",
    "npm:rebuild": "npm run npm:clean && npm run npm:i && npm run npm:fix",
    "npm:i": "npm cache clean -f && rimraf node_modules/ && npm i --legacy-peer-deps",
    "npm:clean": "rimraf node_modules/ && rimraf ./package-lock.json && npm cache clean -f",
    "npm:fix": "npm audit fix",
    "-----ANGULAR-----": "exit 0",
    "ng:lib": "node ./scripts/create-lib.js",
    "-----LIBRARIES-----": "exit 0",
    "build:libs": "node ./scripts/build-libs.js",
    "-----GIT-----": "exit 0",
    "git:sync:branches": "scripts/git-sync.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~18.2.1",
    "@angular/cdk": "~18.2.1",
    "@angular/common": "~18.2.1",
    "@angular/compiler": "~18.2.1",
    "@angular/core": "~18.2.1",
    "@angular/forms": "~18.2.1",
    "@angular/platform-browser": "~18.2.1",
    "@angular/platform-browser-dynamic": "~18.2.1",
    "@angular/router": "~18.2.1",
    "@sea-code-themes/colors": "~3.1.0",
    "@sea-code-themes/spacing": "~3.1.0",
    "@sea-code-themes/typography": "~3.1.0",
    "@eslint/js": "~9.9.1",
    "luxon": "~3.5.0",
    "rxjs": "~7.8.1",
    "tslib": "~2.7.0",
    "zone.js": "~0.14.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.1",
    "@angular-eslint/builder": "~18.3.0",
    "@angular-eslint/eslint-plugin": "~18.3.0",
    "@angular-eslint/eslint-plugin-template": "~18.3.0",
    "@angular-eslint/schematics": "~18.3.0",
    "@angular-eslint/template-parser": "~18.3.0",
    "@angular/cli": "~18.2.1",
    "@angular/compiler-cli": "~18.2.1",
    "@commitlint/cli": "~19.4.0",
    "@commitlint/config-conventional": "~19.2.2",
    "@eslint/compat": "~1.1.1",
    "@eslint/eslintrc": "~3.1.0",
    "@types/jasmine": "~5.1.4",
    "@types/luxon": "~3.4.2",
    "@typescript-eslint/eslint-plugin": "~8.2.0",
    "@typescript-eslint/parser": "~8.0.0",
    "eslint": "~8.57.0",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "fs-extra": "~11.2.0",
    "globals": "~15.9.0",
    "husky": "~9.1.5",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "~15.2.9",
    "ng-packagr": "~18.2.0",
    "prettier": "~3.3.3",
    "ts-loader": "~9.5.1",
    "typescript": "~5.5.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
